{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 60,
      "memory": 2048
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "SHARP_IGNORE_GLOBAL_LIBVIPS": "1"
    }
  }
}